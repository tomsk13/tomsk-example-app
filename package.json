{
  "name": "@ng-state/example-app",
  "version": "1.0.0",
  "description": "Example application demoing the ng-state platform",
  "main": "index.ts",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4202",
    "lint": "tslint \"src/**/*.ts\" --project src/tsconfig.json --type-check && tslint \"e2e/**/*.ts\" --project e2e/tsconfig.json --type-check",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "build": "del-cli dist && ng build",
    "build:prod": "npm run build -- --configuration=prod",
    "build:dev": "ng build",
    "build:server:prod": "ng run example-app:server:prod",
    "compile:server": "tsc -p server.tsconfig.json",
    "build:ssr:prod": "npm run build:prod && npm run build:server:prod && npm run compile:server",
    "serve:ssr": "node dist/server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ng-state/example-app.git"
  },
  "author": "Vytautas Pranskunas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ng-state/example-app/issues"
  },
  "homepage": "https://github.com/ng-state/example-app#readme",
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.1.4",
    "@angular/common": "7.1.4",
    "@angular/compiler": "7.1.4",
    "@angular/core": "7.1.4",
    "@angular/forms": "7.1.4",
    "@angular/http": "7.1.4",
    "@angular/platform-browser": "7.1.4",
    "@angular/platform-browser-dynamic": "7.1.4",
    "@angular/platform-server": "7.1.4",
    "@angular/pwa": "0.13.6",
    "@angular/router": "7.1.4",
    "@angular/service-worker": "7.1.4",
    "@ng-state/immer-data-strategy": "^1.0.0",
    "@ng-state/immutablejs-data-strategy": "^1.0.0",
    "@ng-state/store": "^6.5.2",
    "@nguniversal/common": "7.1.1",
    "@nguniversal/express-engine": "7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "7.1.1",
    "bootstrap": "^4.0.0-alpha.6",
    "core-js": "^2.5.1",
    "domino": "^2.1.3",
    "hammerjs": "^2.0.8",
    "immer": "^3.1.2",
    "immutable": "^3.8.1",
    "immutable-cursor": "^2.0.1",
    "ngx-take-until-destroy": "^5.4.0",
    "rxjs": "~6.3.2",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.13.6",
    "@angular/cli": "7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.42",
    "codelyzer": "~2.0.0-beta.1",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "~4.0.13",
    "ts-node": "1.2.1",
    "tslint": "^4.3.0",
    "typescript": "3.1.6",
    "http-server": "^0.11.1",
    "del-cli": "^1.1.0"
  }
}
